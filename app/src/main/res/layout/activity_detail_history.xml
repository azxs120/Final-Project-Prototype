<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailHistory">


    <TextView
        android:id="@+id/subjectTextView"
        android:layout_width="950px"
        android:layout_height="20dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:gravity="center"
        android:text="Subject"
        android:textAlignment="gravity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/subject"
        android:layout_width="950px"
        android:layout_height="140px"
        android:layout_marginTop="36dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="Subject"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/messageBodyTextView"
        android:layout_width="950px"
        android:layout_height="20dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="24dp"
        android:text="Body"
        android:gravity="center"
        android:textAlignment="gravity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/messageBody"
        android:layout_width="950px"
        android:layout_height="265dp"
        android:layout_marginTop="108dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:gravity="top"
        android:hint="Body"
        android:padding="10dp"
        android:scrollbars="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/startDateTextView"
        android:layout_width="950px"
        android:layout_height="20dp"
        android:layout_marginTop="372dp"
        android:layout_marginEnd="24dp"
        android:text="Start Date"
        android:textAlignment="gravity"
        android:gravity="center"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/startDate"
        android:layout_width="950px"
        android:layout_height="140px"
        android:layout_marginTop="388dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="Start Date"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/endDateTextView"
        android:layout_width="950px"
        android:layout_height="20dp"
        android:layout_marginTop="444dp"
        android:layout_marginEnd="24dp"
        android:gravity="center"

        android:text="End Date"
        android:textAlignment="gravity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/endDate"
        android:layout_width="950px"
        android:layout_height="140px"
        android:layout_marginTop="464dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="End Date"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/homeOwnerCallStatusTextView"
        android:layout_width="950px"
        android:layout_height="20dp"
        android:layout_marginTop="516dp"
        android:gravity="center"

        android:layout_marginEnd="20dp"
        android:text="Home Owner Call Status"
        android:textAlignment="gravity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/homeOwnerCallStatus"
        android:layout_width="950px"
        android:layout_height="140px"
        android:layout_marginTop="536dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="Home Owner Call Status"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.591"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tenantCallStatusTextView"
        android:layout_width="950px"
        android:layout_height="20dp"
        android:layout_marginTop="588dp"
        android:layout_marginEnd="24dp"
        android:gravity="center"

        android:text="Tenant Call Status"
        android:textAlignment="gravity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/tenantCallStatus"
        android:layout_width="950px"
        android:layout_height="140px"
        android:layout_marginTop="608dp"
        android:background="@drawable/textview_border"
        android:ems="10"
        android:hint="Tenant Call Status"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.591"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>