<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:ignore="ExtraText">

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="@string/searchLandlordTenant"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/searchBtn"
        app:layout_constraintTop_toBottomOf="@+id/searchBtn" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/searchApartment"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/homeBtn"
        app:layout_constraintTop_toBottomOf="@+id/homeBtn" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="@string/callHandling"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/callBtn"
        app:layout_constraintTop_toBottomOf="@+id/callBtn" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="@string/myHistory"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/myHistoryBtn"
        app:layout_constraintTop_toBottomOf="@+id/myHistoryBtn" />

    <ImageButton
        android:id="@+id/homeBtn"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="44dp"
        android:contentDescription="@string/btn_to_find_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/house" />

    <ImageButton
        android:id="@+id/searchBtn"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="208dp"
        android:contentDescription="@string/search_home_btn"
        android:layout_marginEnd="52dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/homeBtn"
        app:srcCompat="@mipmap/men_foreground" />

    <ImageButton
        android:id="@+id/callBtn"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="44dp"
        android:contentDescription="@string/call_handling_btn"
        android:layout_marginTop="164dp"
        android:background="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/service_foreground" />

    <ImageButton
        android:id="@+id/myHistoryBtn"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="208dp"
        android:background="#FFFFFF"
        android:contentDescription="@string/my_history_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/callBtn"
        app:srcCompat="@mipmap/clock_foreground" />



</androidx.constraintlayout.widget.ConstraintLayout>